    CHGOBJOWN  OBJ(&TGTLIB/QCLSRC)    OBJTYPE(*FILE) NEWOWN(QMQM)
    CHGOBJOWN  OBJ(&TGTLIB/QCMDSRC)   OBJTYPE(*FILE) NEWOWN(QMQM)
    CHGOBJOWN  OBJ(&TGTLIB/QCSRC)     OBJTYPE(*FILE) NEWOWN(QMQM)
