    CHGOBJOWN  OBJ(&TGTLIB)           OBJTYPE(*LIB)  NEWOWN(QMQM)
    CHGOBJOWN  OBJ(&TGTLIB/BUILDMS03) OBJTYPE(*PGM)  NEWOWN(QMQM)
    CHGOBJOWN  OBJ(&TGTLIB/SAVEQMGR)  OBJTYPE(*PGM)  NEWOWN(QMQM)
    CHGOBJOWN  OBJ(&TGTLIB/QSAVEQMGR) OBJTYPE(*CMD)  NEWOWN(QMQM)
